{
    "name": "pxgamer/xbtit-to-unit3d",
    "type": "library",
    "description": "An artisan package to import a XBTIT database into UNIT3D.",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\XbtitToUnit3d\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\XbtitToUnit3d\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "pxgamer\\XbtitToUnit3d\\ServiceProvider"
            ]
        }
    }
}
